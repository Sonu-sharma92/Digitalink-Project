pipeline {
    agent any

    stages {
        stage('Git code') {
            steps {
                git branch: 'main', url: 'https://github.com/Sonu-sharma92/Digitalink-Project.git'
            }
        }

    stage('Docker-build') {
            steps {
                sh 'docker build -f /Digitalink-Project/Dockerfile1' 
            }
        }

     
    }
}
