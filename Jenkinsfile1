pipeline {
    agent any

    stages {
        stage('Git code') {
            steps {
                git branch: 'main', url: 'https://github.com/Sonu-sharma92/digitalink-Project.git'
            }
        }

    stage('Docker-build and run') {
            steps {
                sh 'docker build -f Dockerfile1 -t digitalink-frontend .' 
                
                sh 'docker run -d -p 80:80 digitalnk-frontend'
            }
        }

     
    }
}
